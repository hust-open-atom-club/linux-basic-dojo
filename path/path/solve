/bin/fold -s <<< "尝试删除 /flag..."
rm -f /flag

if [ -f /flag ]
then
	/bin/fold -s <<< " flag 依然存在！这是你应该得到的！"
	/bin/cat /flag
else
	/bin/fold -s <<< "芜湖，我成功地删除了 flag ！你没有正确设置 PATH，让我找到了 'rm'。因为 flag 不见了，你需要从模块页面重新启动挑战！下次好运。"
fi

